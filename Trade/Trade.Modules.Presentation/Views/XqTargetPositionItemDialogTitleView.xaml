<UserControl x:Class="XueQiaoWaf.Trade.Modules.Presentation.Views.XqTargetPositionItemDialogTitleView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignWidth="300">
    <StackPanel Grid.Row="0" Orientation="Horizontal">
        <TextBlock FontSize="{DynamicResource DialogTitleFontSize}"
                   Text="{Binding TitlePrefix}"/>
        <ContentControl 
            FontSize="{DynamicResource DialogTitleFontSize}"
            Content="{Binding XqTargetPositionItem}" 
            ContentTemplate="{StaticResource XqTargetNameDT}"/>
    </StackPanel>
</UserControl>
